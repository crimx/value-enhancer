{
  "name": "value-enhancer",
  "version": "2.0.0",
  "private": false,
  "description": "A tiny library to enhance value with reactive wrapper.",
  "license": "MIT",
  "author": "CRIMX <straybugs@gmail.com> (https://github.com/crimx/)",
  "repository": "crimx/value-enhancer",
  "sideEffects": false,
  "main": "./dist/value-enhancer.js",
  "module": "./dist/value-enhancer.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "prepublish": "pnpm run build",
    "lint": "eslint --ext .ts,.tsx . && prettier --check .",
    "test": "jest --coverage",
    "docs": "typedoc --cname value-enhancer.js.org --includeVersion --excludePrivate --excludeProtected --excludeInternal --out docs src/index.ts",
    "types": "cross-env NODE_ENV=production tsc --declaration --emitDeclarationOnly --jsx react --esModuleInterop --outDir dist",
    "build": "vite build && npm run types",
    "build:dev": "vite build --mode development && npm run types",
    "release": "standard-version"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.3",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^28.1.3",
    "prettier": "^2.4.0",
    "standard-version": "^9.3.1",
    "ts-jest": "^28.0.8",
    "typedoc": "^0.23.15",
    "typescript": "^4.4.3",
    "vite": "^3.0.9"
  }
}
